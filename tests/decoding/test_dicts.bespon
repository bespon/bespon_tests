# Test dicts


|=== test_indentation_dict
status = valid
bespon =
  * |```
    key1 = value1
    key2 = value2
    |```/
  * |```
      key1 = value1
      key2 = value2
    |```/
  * |'''
    \tkey1\t=\tvalue1
    \tkey2\t=\tvalue2
    |'''/
json = `{"key1": "value1", "key2": "value2"}`


|=== test_nested_indentation_dict
status = valid
bespon =
  * |```
    key1 =
      key2 =
        key3 = value
    k = v
    |```/
json = `{"key1": {"key2": {"key3": "value"}}, "k": "v"}`


|=== test_inline_dict
status = valid
bespon =
  * |```
    {key1 = {key2 = {key3 = value}}, k = v}
    |```/
  * |```
    {
     key1 = {key2 = {key3 = value}}, k = v}
    |```/
  * |```
    {key1 =
    {key2 = {key3 = value}}, k = v}
    |```/
  * |```
    {key1 =
        {key2 =
    {key3 = value}}, k = v}
    |```/
json = `{"key1": {"key2": {"key3": "value"}}, "k": "v"}`


|=== test_empty_inline_dict
status = valid
bespon = ['{}', ' {}', '{ }', '{\t}']
json = '{}'


